{
  "headers": [
    {
      "source": "/font/(.*).ttf",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=3600, maxage=3600, must-revalidate, stale-while-revalidate=300, stale-if-error=86400"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/status.json",
      "destination": "https://raw.githubusercontent.com/equestriadev-status/realtime/master/status.json",
      "permanent": true
    }
  ]
}
